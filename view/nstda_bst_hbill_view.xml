		<!-- TreeView prj Report -->
		<record model="ir.ui.view" id="view_nstda_bst_hbill_prjreport_tree">
			<field name="name">nstda.bst.hbill.prjreport.tree</field>
			<field name="model">nstda.bst.hbill</field>
